{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "esmodules": true
                }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        [
                "import",
                {
                    "libraryName": "antd",
                    "style": "css"
                }
            ],
        [
          "@babel/plugin-proposal-class-properties"
        ]
    ]
}